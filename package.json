{
  "name": "dynamo-cli",
  "version": "0.0.9",
  "description": "dynamo operations from command line",
  "main": "lib/index.js",
  "repository": "git@github.com:kanekotic/dynamo-cli.git",
  "author": "kanekotic <alvarojosepl@gmail.com>",
  "license": "MIT",
  "bin": {
    "dynamo-cli": "bin/dynamo-cli.js"
  },
  "keywords": [
    "command line",
    "tool",
    "dynamodb",
    "batch",
    "csv"
  ],
  "scripts": {
    "test": "jest",
    "test:dev": "jest --watchAll",
