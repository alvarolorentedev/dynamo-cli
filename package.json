    "deploy:minor": "yarn version --minor",
    "deploy:patch": "yarn version --patch",
    "preversion": "yarn test"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "coveralls": "3.1.1",
    "jest": "29.3.1"
  },
  "dependencies": {
    "aws-sdk": "2.1267.0",
    "commander": "9.4.1",
    "csv-parse": "5.3.3"
  },
  "jest": {
    "automock": false,
    "testMatch": [
      "**/test/**/*-test.js"
    ]
  }
}
